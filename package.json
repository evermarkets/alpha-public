{
  "name": "evermarkets_alpha",
  "version": "0.0.1",
  "description": "Alpha release of EverMarkets Server",
  "main": "server/index.js",
  "scripts": {
    "truffle": "truffle",
    "test": "truffle test --network test",
    "compile": "truffle compile --all",
    "migrate": "truffle migrate --reset",
    "networks": "truffle networks",
    "coverage": "./node_modules/.bin/solidity-coverage && open ./coverage/index.html",
    "prep_es": "truffle exec debug/prep_es.js",
    "injectnetworks": "node debug/inject_networks.js",
    "extractnetworks": "node debug/extract_networks.js",
    "eslint": "eslint . --ext .js,.jsx",
    "prebuild": "rimraf ./public/dist/",
    "testrpc:dev": "NODE_ENV=devserver node common/testrpc-start-migrate.js",
    "build": "NODE_ENV=production webpack --config webpack.production.config.js --history-api-fallback --progress --colors",
    "build:devserver": "NODE_ENV=devserver webpack-dev-server --config webpack.devserver.config.js --host 0.0.0.0 --inline --progress --content-base public/dist/ | grep -v  \"\\[built\\]\"",
    "start": "NODE_ENV=production node server/index.js",
    "start:devserver": "NODE_ENV=devserver node server/index.js",
    "deploy": "make clean; make eb_deploy.zip; eb use evermarkets; eb deploy --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evermarkets/alpha.git"
  },
